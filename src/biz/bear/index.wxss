*,
*:before,
*:after {
  /* border box */
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  /* gpu acceleration */
  -webkit-transition: translate3d(0, 0, 0);
  /* select disable */
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
#gamecontainer {
  position: relative;
  width: 100%;
  height: 100%;
  min-height: 28.45866667rem;
}
/*
 Screen - Game
 */
#gamescreen {
  position: absolute;
  width: 100%;
  height: 100%;
}
#sky {
  position: absolute;
  top: 0;
  width: 100%;
  height: 80%;
  background-image: url('../../../assets/static/imgs/bear/sky.png');
  background-repeat: repeat-x;
  background-position: 0px 100%;
  background-color: #4ec0ca;
  -webkit-animation: animSky 7s linear infinite;
  animation: animSky 7s linear infinite;
}
#flyarea {
  position: absolute;
  bottom: 0;
  height: 17.92rem;
  width: 100%;
}
#ceiling {
  position: absolute;
  top: -0.68266667rem;
  height: 0.68266667rem;
  width: 100%;
  background-image: url('../../../assets/static/imgs/bear/ceiling.png');
  background-repeat: repeat-x;
  -webkit-animation: animCeiling 481ms linear infinite;
  animation: animCeiling 481ms linear infinite;
}
#land {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 20%;
  background-image: url('../../../assets/static/imgs/bear/land.png');
  background-repeat: repeat-x;
  background-position: 0px 0px;
  background-color: #ded895;
  -webkit-animation: animLand 2516ms linear infinite;
  animation: animLand 2516ms linear infinite;
}
#bigscore {
  position: absolute;
  top: 0.42666667rem;
  left: 3.2rem;
  z-index: 100;
}
#bigscore img {
  display: inline-block;
  padding: 0.02133333rem;
}
#splash {
  position: absolute;
  top: 1.6rem;
  left: 1.38666667rem;
  width: 4.01066667rem;
  height: 3.62666667rem;
  background-image: url('../../../assets/static/imgs/bear/splash.png');
  background-repeat: no-repeat;
}
#scoreboard {
  position: absolute;
  display: none;
  opacity: 1;
  top: 1.36533333rem;
  left: 0.91733333rem;
  width: 5.03466667rem;
  height: 5.97333333rem;
  background-image: url('../../../assets/static/imgs/bear/scoreboard.png');
  background-repeat: no-repeat;
  z-index: 1000;
}
#medal {
  position: absolute;
  opacity: 0;
  top: 2.432rem;
  left: 0.68266667rem;
  width: 0.93866667rem;
  height: 0.93866667rem;
}
#currentscore {
  position: absolute;
  top: 2.24rem;
  left: 2.28266667rem;
  width: 2.21866667rem;
  height: 0.29866667rem;
  text-align: right;
}
#currentscore img {
  padding-left: 0.04266667rem;
}
#highscore {
  position: absolute;
  top: 3.136rem;
  left: 2.28266667rem;
  width: 2.21866667rem;
  height: 0.29866667rem;
  text-align: right;
}
#highscore img {
  padding-left: 0.04266667rem;
}
#replay {
  position: absolute;
  opacity: 0;
  top: 4.37333333rem;
  left: 1.30133333rem;
  height: 2.45333333rem;
  width: 1.49333333rem;
  cursor: pointer;
}
.boundingbox {
  position: absolute;
  display: none;
  top: 0;
  left: 0;
  width: 0;
  height: 0;
  border: 1px solid red;
}
#player {
  left: 1.28rem;
  top: 4.26666667rem;
}
.bird {
  position: absolute;
  width: 0.72533333rem;
  height: 0.512rem;
  background-image: url('../../../assets/static/imgs/bear/bird.png');
  -webkit-animation: animBird 300ms steps(4) infinite;
  animation: animBird 300ms steps(4) infinite;
}
.pipe {
  position: absolute;
  left: -2.13333333rem;
  width: 1.10933333rem;
  height: 100%;
  z-index: 10;
  -webkit-animation: animPipe 7500ms linear;
  animation: animPipe 7500ms linear;
}
.pipe_upper {
  position: absolute;
  top: 0;
  width: 1.10933333rem;
  background-image: url('../../../assets/static/imgs/bear/pipe.png');
  background-repeat: repeat-y;
  background-position: center;
}
.pipe_upper:after {
  content: "";
  position: absolute;
  bottom: 0;
  width: 1.10933333rem;
  height: 0.55466667rem;
  background-image: url('../../../assets/static/imgs/bear/pipe-down.png');
}
.pipe_lower {
  position: absolute;
  bottom: 0;
  width: 1.10933333rem;
  background-image: url('../../../assets/static/imgs/bear/pipe.png');
  background-repeat: repeat-y;
  background-position: center;
}
.pipe_lower:after {
  content: "";
  position: absolute;
  top: 0;
  width: 1.10933333rem;
  height: 0.55466667rem;
  background-image: url('../../../assets/static/imgs/bear/pipe-up.png');
}
#footer {
  position: absolute;
  bottom: 0.064rem;
  left: 0.064rem;
}
#footer a,
#footer a:link,
#footer a:visited,
#footer a:hover,
#footer a:active {
  display: block;
  padding: 0.04266667rem;
  text-decoration: none;
  color: #fff;
}
/*动画特效*/
.fade-splash-enter-active {
  transition: opacity 1s ease;
}
.fade-splash-leave-active {
  transition: opacity 0.5s ease;
}
.fade-splash-enter,
.fade-splash-leave-to {
  opacity: 0;
}
